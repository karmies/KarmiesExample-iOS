{
  "name": "KarmiesSDK",
  "module_name": "KarmiesSDK",
  "version": "2.4.3",
  "license": {
    "text": "Copyright 2016 Karmies, Inc.",
    "type": "Commercial"
  },
  "homepage": "https://karmies.com",
  "authors": {
    "Karmies": "support@karmies.com"
  },
  "summary": "Karmies SDK in Swift for iOS and macOS",
  "source": {
    "git": "https://github.com/karmies/karmies-sdk-cocoa.git",
    "tag": "2.4.3"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "8.0",
    "osx": "10.12"
  },
  "ios": {
    "source_files": [
      "KarmiesSDK/**/*.{swift,h,m}",
      "Pods/GoogleAnalytics/Sources/*.h"
    ],
    "public_header_files": [
      "KarmiesSDK/**/*.h",
      "Pods/GoogleAnalytics/Sources/*.h"
    ],
    "vendored_libraries": "Pods/GoogleAnalytics/Libraries/libGoogleAnalytics.a",
    "frameworks": [
      "UIKit",
      "CoreData",
      "WebKit"
    ],
    "dependencies": {
      "AwesomeCache": [
        "~> 5.0"
      ],
      "FastImageCache": [
        "~> 1.5"
      ],
      "AWSMobileAnalytics": [
        "~> 2.5"
      ]
    }
  },
  "osx": {
    "source_files": "KarmiesSDK/**/*.{swift,h,m}"
  },
  "libraries": [
    "z",
    "sqlite3"
  ],
  "resource_bundles": {
    "KarmiesResources": [
      "KarmiesSDK/Resources/*.xcassets",
      "KarmiesSDK/Resources/*.storyboard"
    ]
  },
  "dependencies": {
    "Alamofire": [
      "~> 4.0"
    ],
    "AlamofireImage": [
      "~> 3.1"
    ],
    "ObjectMapper": [
      "~> 2.1"
    ],
    "GBDeviceInfo": [
      "~> 4.2"
    ],
    "CKCircleMenuView": [
      "~> 0.3"
    ]
  }
}
